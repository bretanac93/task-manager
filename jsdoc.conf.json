{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "source": {
      "include": ["src"],
      "exclude": ["src/routes"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
  },
  "opts": {
      "destination": "docs/app",
      "recurse": true,
      "readme": "README.md"
  }
}